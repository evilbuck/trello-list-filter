{
  "name": "Trello List Filter",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "Filter Lists on Trello Boards",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://trello.com/*",
        "https://trello.com/*"
      ],
      "css": [
        "stylesheets/content.css"
      ],
      "js": [
        "vendor/jquery.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}